extends: yoast

parserOptions:
  ecmaVersion: 2018

settings:
  react:
    version: "16.6"

overrides:
  - files:
      - "packages/analysis-report/**/*.js"
    rules:
      react/jsx-no-bind: 1
      react/no-unused-state: 1
      react/no-access-state-in-setstate: 1
      react/require-default-props: 1
      react/default-props-match-prop-types: 1
